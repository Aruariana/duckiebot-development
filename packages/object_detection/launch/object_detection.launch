<launch>
    <arg name="veh" default="$(env VEHICLE_NAME)"/>
    <arg name="pkg_name" value="object_detection"/>
    <arg name="node_name" value="object_detection_node"/>

    <group ns="$(arg veh)">
        <node pkg="$(arg pkg_name)" type="$(arg node_name).py" name="$(arg node_name)" output="screen">
            <remap from="~image/compressed" to="camera_node/image/compressed"/>
            <remap from="~camera_info" to="camera_node/camera_info"/>
        </node>
    </group>
</launch>